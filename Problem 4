#include<iostream>
	using namespace std;
	int main()
	{
	    double number1;
	    double number2;
	    double number3;
	    
	    cout<< "Enter first number: ";
	    cin>> number1;
	    cout<< "Enter second number: ";
	    cin>> number2;
	    cout<< "Enter third number: ";
	    cin>> number3;
	    
	    if(number1 >= number2 && number1 >= number3)
        cout << "The LARGEST of the three is " << number1;
        else if(number2 >= number1 && number2 >= number3)
        cout << "The LARGEST of the three is " << number2;
        else if(number3 >= number1 && number3 >= number2)
        cout << "The LARGEST of the three is " << number3;
     
	    return 0;
		}
